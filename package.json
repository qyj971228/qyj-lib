    "jest-css-modules": "^2.1.0",
    "node-sass": "^7.0.1",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "react-docgen-typescript-plugin": "^1.0.5",
    "react-test-renderer": "^18.2.0",

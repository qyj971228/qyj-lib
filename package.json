{
  "name": "qyj-lib",
  "version": "0.1.8",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf ./dist",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-sass": "node-sass ./src -o ./dist --output-style expanded",
    "watch-sass": "node-sass --watch ./src -o ./src --output-style expanded",
    "init-sass": "node-sass ./src -o ./src --output-style expanded",
    "init-watch-sass": "npm run init-sass && npm run watch-sass",
    "build": "npm run clean && npm run build-sass && npm run build-ts",
    "build-publish": "npm run clean && npm run build-sass && npm run build-ts && npm publish"
  },
  "devDependencies": {
    "node-sass": "^7.0.1",
    "sass-loader": "^13.0.2",
    "typescript": "^4.7.4"
  },
  "browserslist": [
    "> 0.2%",
    "defaults ",
    "not IE 11",
    "not op_mini all"
  ]
}
